{
  "name": "path-fast",
  "version": "1.1.0",
  "description": "A CLI tool designed to streamline access to projects.",
  "main": "dist/index.js",
  "type": "module",
  "private": false,
  "bin": {
    "pf": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "docs"
  ],
  "scripts": {
    "build": "tsc && cp package.json dist/",
    "dev": "tsc --watch",
    "dev:pf": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "new:version": "npm version patch -m 'release: %s' && git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/path-fast/core.git"
  },
  "bugs": {
    "url": "https://github.com/path-fast/core/issues"
  },
  "author": "Eduardo Nicola eduardonicolagit@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/path-fast/core#readme",
  "keywords": [
    "cli",
    "path",
    "shortcut",
    "project-management",
    "automation",
    "vscode"
  ],
  "dependencies": {
    "commander": "^13.1.0",
    "inquirer": "^12.11.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.3",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.26.2"
}
